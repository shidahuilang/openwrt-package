
config rule 'mainland_ip'
	option proto 'all'
	option sticky '0'
	option ipset 'ip_mainland'
	option use_policy 'default'

config rule 'mainland_net'
	option proto 'all'
	option sticky '0'
	option ipset 'net_mainland'
	option use_policy 'default'

config rule 'hongkong_ip'
	option proto 'all'
	option sticky '0'
	option ipset 'ip_hongkong'
	option use_policy 'hongkong'

config rule 'hongkong_net'
	option proto 'all'
	option sticky '0'
	option ipset 'net_hongkong'
	option use_policy 'hongkong'

config rule 'oversea_ip'
	option proto 'all'
	option sticky '0'
	option ipset 'ip_oversea'
	option use_policy 'oversea'

config rule 'oversea_net'
	option proto 'all'
	option sticky '0'
	option ipset 'net_oversea'
	option use_policy 'oversea'

config globals 'globals'
	option mmx_mask '0x3F00'
	option rtmon_interval '5'

config interface 'vpnhub01'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	option enabled '1'

config interface 'vpnhub02'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	option enabled '1'

config member 'hub01'
	option interface 'vpnhub01'

config member 'hub02'
	option interface 'vpnhub02'

config policy 'hongkong'
	list use_member 'hub01'
	option last_resort 'unreachable'

config policy 'oversea'
	list use_member 'hub02'
	option last_resort 'unreachable'

