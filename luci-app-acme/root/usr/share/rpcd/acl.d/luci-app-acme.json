{
        "luci-app-acme": {
                "description": "Grant UCI access for luci-app-acme",
                "read": {
                        "file": {
                                "/etc/ssl/acme": [ "list" ]
                        },
                        "uci": [ "acme" ]
                },
                "write": {
                        "uci": [ "acme" ]
                }
        }
}
