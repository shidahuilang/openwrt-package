{
	"luci-app-simple-adblock": {
		"description": "Grant UCI and file access for luci-app-simple-adblock",
		"read": {
			"cgi-io": [
				"exec"
			],
			"file": {
				"/usr/lib/opkg/status": [
					"read"
				],
				"/sys/class/leds/*": [
					"read"
				],
				"/var/run/simple-adblock.*": [
					"read"
				],
				"/etc/init.d/simple-adblock *": [
					"exec"
				],
				"/usr/sbin/dnsmasq *": [
					"exec"
				],
				"/usr/sbin/unbound *": [
					"exec"
				],
				"/usr/sbin/ipset *": [
					"exec"
				]
			},
				"uci": [
				"simple-adblock"
			]
		},
		"write": {
			"uci": [
				"simple-adblock"
			]
		}
	}
}