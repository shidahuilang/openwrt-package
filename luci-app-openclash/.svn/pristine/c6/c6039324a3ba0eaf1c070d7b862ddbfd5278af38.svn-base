<%+cbi/valueheader%>
<div>
<%
	local val = self:cfgvalue(section)
	if val == translate("Enable") or val == translate("Config Normal") or val == translate("Exist") then
%>
     <div style="color: green; font-weight:bold;">
<%
  else
%>	
     <div style="color: red; font-weight:bold;">
<%	
  end
  write(pcdata(val))
%>
</div>
</div>
<%+cbi/valuefooter%>
