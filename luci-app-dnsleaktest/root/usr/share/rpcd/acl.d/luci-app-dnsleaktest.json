{
	"luci-app-dnsleaktest": {
		"description": "Grant access to luci-app-dnsleaktest",
		"read": {
			"file": {
				"/usr/bin/curl": [ "exec" ],
				"/bin/ping": [ "exec" ],
				"/etc/dnsleaktest/*": [ "read" ]
			}
		},
		"write": {
			"file": {
				"/etc/dnsleaktest/*": [ "write" ]
			}
		}
	}
}