{
	"luci-app-modeminfo": {
		"description": "Grant access to modeminfo configuration",
		"read": {
			"ubus": {
				"file": [ "read" ],
				"luci": [ "getConntrackHelpers" ]
			},
			"file": {
				"/usr/bin/modeminfo": [ "exec" ],
				"/usr/share/modeminfo/scripts/rmtmp": [ "exec" ]
			},
			"uci": [ "modeminfo" ]
		},
		"write": {
			"ubus": {
				"file": [ "write" ]
			},
		
			"uci": [ "modeminfo" ]
		}
	}
}
