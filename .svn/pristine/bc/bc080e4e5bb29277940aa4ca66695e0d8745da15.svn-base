# openconnect avanced configuration

menu "Configuration"
	depends on PACKAGE_openconnect

choice
	prompt "SSL library"
	default OPENCONNECT_GNUTLS

config OPENCONNECT_GNUTLS
	bool "GnuTLS support"

config OPENCONNECT_OPENSSL
	bool "OpenSSL"

endchoice

config OPENCONNECT_STOKEN
	bool "stoken support"

endmenu
