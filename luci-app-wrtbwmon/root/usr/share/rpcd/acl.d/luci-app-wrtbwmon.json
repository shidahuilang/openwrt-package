{
	"luci-app-wrtbwmon": {
		"description": "Grant access to LuCI app wrtbwmon",
		"read": {
			"ubus": {
				"luci.wrtbwmon": [
					"get_db_raw",
					"get_db_path"
				],
				"luci-rpc": [
					"getDHCPLeases",
					"getDSLStatus"
				]
			},
			"file": {
				"/etc/wrtbwmon.user": [ "read" ],
				"/usr/sbin/wrtbwmon": [ "exec" ],
				"/etc/luci-wrtbwmon.conf": [ "read" ]
			},
			"uci": [ "wrtbwmon" ]
		},
		"write": {
			"ubus": {
				"luci.wrtbwmon": [
					"remove_db",
					"change_db_path"
				]
			},
			"file": {
				"/etc/luci-wrtbwmon.conf": [ "write" ],
				"/etc/wrtbwmon.user": [ "write" ]
			},
			"uci": [ "wrtbwmon" ]
		}
	}
}
