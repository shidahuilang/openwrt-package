
config wifi-device 'radio0'
        option type 'mac80211'
        option hwmode '11a'
        option path 'pci0000:00/0000:00:00.0'
        option htmode 'VHT20'
        option txpower '18'
        option cell_density '0'
        option beacon_int '50'
        option distance '30'
        option country 'HU'
        option channel '60'
        option log_level '1'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option mode 'ap'
        option key 'intranetintranetintranet'
        option ssid 'intranet'
        option encryption 'psk2'
        option ft_psk_generate_local '1'
        option mobility_domain '4f4f'
        option country_code 'HU'
        option country3 '0x49'
        option time_advertisement '2'
        option ieee80211d '1'
        option reassociation_deadline '1000'
        option ieee80211r '1'
        option network 'intranet'
        option ieee80211k '1'
        option ieee80211v '1'
        option ft_over_ds '0'
        option na_mcast_to_ucast '1'
        option bss_transition '0'
        option nasid 'ap-10.intranet'

config wifi-device 'radio1'
        option type 'mac80211'
        option hwmode '11g'
        option path 'platform/ahb/18100000.wmac'
        option htmode 'HT20'
        option txpower '16'
        option cell_density '0'
        option distance '30'
        option beacon_int '50'
        option country 'HU'
        option channel '1'
        option log_level '1'

config wifi-iface 'default_radio1'
        option device 'radio1'
        option mode 'ap'
        option key 'intranetintranetintranet'
        option ssid 'intranet'
        option encryption 'psk2'
        option ft_psk_generate_local '1'
        option mobility_domain '4f4f'
        option country_code 'HU'
        option country3 '0x49'
        option time_advertisement '2'
        option reassociation_deadline '1000'
        option ieee80211r '1'
        option network 'intranet'
        option ieee80211k '1'
        option ieee80211v '1'
        option bss_transition '0'
        option na_mcast_to_ucast '1'
        option ft_over_ds '0'
        option nasid 'ap-10.intranet'

config wifi-iface 'wifinet3'
        option device 'radio1'
        option mode 'ap'
        option ssid 'guest'
        option encryption 'psk2'
        option key 'Randompassword2'
        option network 'guest'
        option ft_psk_generate_local '1'
        option country_code 'HU'
        option country3 '0x49'
        option time_advertisement '2'
        option reassociation_deadline '1000'
        option ieee80211r '1'
        option ieee80211k '1'
        option ieee80211v '1'
        option bss_transition '0'
        option na_mcast_to_ucast '1'
        option ft_over_ds '0'
        option nasid 'ap-10.guestnet'
        option mobility_domain 'abab'


