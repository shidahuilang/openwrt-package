#!/usr/sbin/nft -f

table inet mihomo {
	set reserved_ip6 {
		type ipv6_addr
		flags interval
		elements = {
			::/128,
			::1/128,
			::ffff:0:0/96,
			100::/64,
			64:ff9b::/96,
			2001::/32,
			2001:10::/28,
			2001:20::/28,
			2001:db8::/32,
			2002::/16,
			fc00::/7,
			fe80::/10,
			ff00::/8
		}
	}
}
