opengt
 set com 115200n81
 set comecho off
 set senddelay 0.02
 waitquiet 0.2 0.2

 send "at+cmgf=0^m"
 waitfor 5 "OK"
 if % = -1 goto error

 send "at+cscs=\"IRA\"^m"
 waitfor 5 "OK"
 if % = -1 goto error

 send "AT+CUSD=1,\"AA182CA682C546\",15^m"
 waitfor 5 "+CUSD:"
 if % = -1 goto error

 get 1 "^m" $s
 print $s

 exit 0

:error
 print ""

 exit 0
