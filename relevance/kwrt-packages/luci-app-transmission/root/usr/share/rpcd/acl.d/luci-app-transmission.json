{
	"luci-app-transmission": {
		"description": "Grant UCI access for luci-app-transmission",
		"read": {
			"file": {
				"/etc/group": [ "read" ],
				"/etc/passwd": [ "read" ],
				"/usr/share/transmission/web/index.html": [ "list" ]
			},
			"ubus": {
				"file": [ "stat" ],
				"service": [ "list" ]
			},
			"uci": [ "transmission" ]
		},
		"write": {
			"uci": [ "transmission" ]
		}
	}
}
