location /etebase/static {
	access_log off;
	error_log /dev/null;
	expires 1y;
	try_files $uri $uri/ =404;
}

location /etebase/media {
	access_log off;
	error_log /dev/null;
	expires 1y;
	try_files $uri $uri/ =404;
}

location /etebase {
	access_log off;
	error_log /dev/null;
	include uwsgi_params;
	uwsgi_pass unix:///var/run/etebase.socket;
}
